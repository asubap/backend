create table public.announcements (
  id bigint generated by default as identity not null,
  created_at timestamp with time zone not null default now(),
  title text null,
  body text null,
  created_by_user uuid null,
  constraint anouncements_pkey primary key (id),
  constraint anouncements_name_key unique (title),
  constraint anouncements_created_by_user_fkey foreign KEY (created_by_user) references auth.users (id)
) TABLESPACE pg_default;